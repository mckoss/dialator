{
  "manifest_version": 2,
  "name": "Time Dialator",
  "version": "0.1",

  "description": "This extension speeds up time in this web page.",
  "icons": {
    "128": "images/clock-128.png"
  },

  "permissions": [
    "activeTab",
    "declarativeContent"
  ],

  "page_action": {
    "default_icon": {
      "38": "images/clock-128.png"
    },
    "default_title": "Time Dialator"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "web_accessible_resources": [
    "injected.js"
  ]
}
