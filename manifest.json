{
  "manifest_version": 2,
  "name": "Time Dialator",
  "version": "0.1",

  "description": "This extension speeds up time in this web page.",
  "icons": {
    "128": "images/clock-128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://elearning.boatus.org/*/course.html"],
      "all_frames": true,
      "js": ["content-script.js"]
    }
  ],

  "web_accessible_resources": [
    "injected.js"
  ]
}
